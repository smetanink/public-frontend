{
  "name": "public-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "format": "npx --no-install lint-staged",
    "prettier": "prettier --write .",
    "lint": "eslint src",
    "fix:lint": "eslint src --fix",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start:prod": "serve -s build -p 3002",
    "test:logic": "NODE_ENV=test jest",
    "test:ui": "react-scripts test",
    "update:dependency:versions": "ncu -u"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "7.21.10",
    "@faker-js/faker": "8.0.2",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.5.2",
    "@types/node": "20.2.5",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "eslint": "8.42.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "lint-staged": "13.2.2",
    "npm-check-updates": "16.10.12",
    "prettier": "2.8.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.2",
    "react-router-dom": "6.3.0",
    "react-scripts": "5.0.1",
    "serve": "14.2.0",
    "source-map-explorer": "2.5.3",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "web-vitals": "3.3.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
